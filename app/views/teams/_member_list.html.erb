<div class="module team-members">
  <div class="module-header">
    <h5><%= t("views.#{account_type}.teams.show.members_subheading") %></h5>
  </div>

  <div class="module-body">
    <ul class="team_members">
      <%= render partial: "teams/member", collection: @team.students %>
    </ul>

    <%= render "teams/invite_students" %>
    <%= render "teams/pending_invites", invites: @team.pending_invites %>
  </div>
</div>
