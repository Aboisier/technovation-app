<%= content_tag :div,
  class: 'submission-screenshots-edit',
  id: 'submission-screenshots-edit',
  data: {
    object_name: "team_submission",
    update_url: send("#{current_account.type_name}_team_submission_path",
                     submission,
                     team_id: params[:team_id])
  } do %>

  <ul id="edit-screenshots-wrapper">
    <%= render partial: 'student/screenshots/screenshot',
      collection: submission.screenshots,
      locals: { edit: true } %>
  </ul>
<% end %>
