<div class="student-dashboard">
  <div class="flex-row">
    <div class="flex-col-md-6">
      <h1 class="appy-title">
        <%= t("views.application.student_dashboard") %>

        <small>
          <%= render 'pre_program_survey', type: :student %>
        </small>
      </h1>
    </div>

    <div class="flex-col-md-6 align-right">
      <h3>Submission Deadline &ndash; Wednesday, April 26th</h3>
    </div>
  </div>

  <% if current_student.full_access_enabled? %>
    <%= render 'student/dashboards/full_access' %>
  <% else %>
    <%= render 'student/dashboards/to_do' %>
  <% end %>
</div>
